[ignore]
.*/node_modules/.*
.*/build/.*
.*/config/.*
.*/public/.*
.*/scripts/.*
.*/flow-typed/.*
.*/.history/.*
[include]
packages/.*/src
[libs]
flow-typed/
packages/math/interface
packages/theme-config/interface
packages/theme-core/interface
packages/theme-css-hoc/interface
packages/theme-hoc/interface
node_modules/@lugia/jverify/interface
node_modules/@lugia/type-utils/interface
node_modules/@lugia/react-test-utils/interface
