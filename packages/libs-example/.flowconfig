[ignore]
.*/node_modules/.*
.*/build/.*
.*/config/.*
.*/public/.*
.*/scripts/.*
.*/flow-typed/.*
.*/.history/.*
[include]
packages/.*/src
[libs]
flow-typed/
interface/
../../flow-typed/

node_modules/@lugia/theme-config/interface
node_modules/@lugia/theme-css-hoc/interface
node_modules/@lugia/theme-hoc/interface
node_modules/@lugia/theme-utils/interface
node_modules/@lugia/theme-hoc-devtools/interface
[options]
module.name_mapper='^core/\(.*\)$' -> '<PROJECT_ROOT>/src/core/\1'
